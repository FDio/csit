# Most of the content of this file has been copied from:
# http://stdio.be/vpp/gdb-commands.txt

# Usage:
# gdb ${BINFILE} ${CORE} -ex 'source -v gdb-commands' -ex quit

set pagination off
bt

define printstack
  set $i=0
  while $i < 15
      frame $i
      x/i $pc
      info locals
      info reg
      set $i = $i + 1
  end
end
printstack

# info proc mappings
