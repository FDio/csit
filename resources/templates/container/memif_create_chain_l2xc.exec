create memif socket id $sid1 filename $socket1
create interface memif id $mid1 socket-id $sid1 slave ring-size 512 rx-queues 3 tx-queues 3
set int state memif$sid1/$mid1 up

create memif socket id $sid2 filename $socket2
create interface memif id $mid2 socket-id $sid2 slave ring-size 512 rx-queues 3 tx-queues 3
set int state memif$sid2/$mid2 up

test l2patch rx memif$sid2/$mid2 tx memif$sid1/$mid1
test l2patch rx memif$sid1/$mid1 tx memif$sid2/$mid2
