# Most of the content of this file has been copied from:
# http://stdio.be/vpp/gdb-commands.txt

# Usage:
# gdb ${BINFILE} ${CORE} -ex 'source -v gdb-commands' -ex quit

set pagination off
thread apply all bt

define printstack
  frame 8
  x/i $pc
  disassemble clib_memcpy16
  x/15i $pc-35
  info locals
  info reg
end
thread apply 1 printstack

# info proc mappings
