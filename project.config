[access]
	inheritFrom = All-Projects
[project]
	description = Integration tests
[access "refs/*"]
	owner = group ldap/fdio-gerrit-csit-committers
[submit]
	action = rebase if necessary
[access "refs/heads/csit-verified"]
	push = group ldap/fdio-gerrit-csit-committers
	pushMerge = group ldap/fdio-gerrit-csit-committers
[access "refs/tags/*"]
	push = group Non-Interactive Users
